{
	"name": "svelte-electron",
	"version": "0.0.1",
	"scripts": {
		"dev": "concurrently --kill-others -n=Svelte,Electron -c=red,blue \"vite\" \"electron src/electron/main.js\"",
		"vite:dev": "vite",
		"vite:build": "vite build",
		"vite:preview": "vite preview",
		"electron": "electron src/electron/main.js",
		"simulate": "vite build && cross-env SIMULATE=1 electron src/electron/main.js"
	},
	"devDependencies": {
		"@sveltejs/vite-plugin-svelte": "1.3.1",
		"concurrently": "7.6.0",
		"cross-env": "^7.0.3",
		"electron": "21.3.1",
		"electron-reload": "2.0.0-alpha.1",
		"page": "1.11.6",
		"svelte": "3.53.1",
		"vite": "3.2.4"
	},
	"dependencies": {
		"electron-serve": "^1.1.0"
	}
}
